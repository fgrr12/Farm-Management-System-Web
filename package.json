{
	"name": "cattle",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --mode local-develop",
		"prod": "vite --mode production",

		"build:production": "tsc && vite build --mode production",
		"build:develop": "tsc && vite build --mode develop",
		"preview": "vite preview",

		"test": "vitest",

		"lint": "biome check . --write",
		"format": "biome format --write .",
		"lint:check": "biome check .",
		"format:check": "biome format .",

		"i18n:scan": "i18next-scanner --config i18next-scanner.config.cjs",
		"i18n:format": "biome format public/locales --write",
		"i18n": "npm run i18n:scan && npm run i18n:format"
	},
	"dependencies": {
		"@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
		"@floating-ui/react": "^0.27.13",
		"@gsap/react": "^2.1.2",
		"dayjs": "^1.11.13",
		"firebase": "^11.10.0",
		"gsap": "^3.13.0",
		"i18next": "^25.3.0",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-http-backend": "^3.0.2",
		"react": "^19.1.0",
		"react-day-picker": "^9.7.0",
		"react-dom": "^19.1.0",
		"react-i18next": "^15.6.0",
		"react-router-dom": "7.6.3",
		"zustand": "5.0.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "24.0.10",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react-swc": "^3.10.2",
		"daisyui": "^5.0.43",
		"i18next-scanner": "^4.6.0",
		"jsdom": "^26.1.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"unocss": "^66.3.2",
		"vite": "^7.0.1",
		"vitest": "^3.2.4"
	}
}
